{
  "name": "rallia",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "mobile": "npm run dev --workspace=@rallia/mobile",
    "mobile:android": "npm run android --workspace=@rallia/mobile",
    "mobile:ios": "npm run ios --workspace=@rallia/mobile",
    "web": "npm run dev --workspace=@rallia/web",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate-types": "npx supabase gen types typescript --project-id YOUR_PROJECT_ID > packages/shared-types/src/supabase-generated.ts",
    "db:studio": "npx supabase start --workdir .",
    "db:reset": "npx supabase db reset --workdir ."
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-export-default-from": "^7.27.1",
    "@babel/plugin-syntax-flow": "^7.27.1",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/traverse": "7.23.2",
    "@expo/fingerprint": "^0.15.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-preset-react-app": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "metro": "^0.83.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.5",
    "turbo": "^2.3.0",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "prettier-plugin-tailwindcss": "^0.5.14"
  }
}
